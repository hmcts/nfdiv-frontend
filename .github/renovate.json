{
  "enabledManagers": ["helm-requirements", "nvm", "terraform"],
  "labels": ["dependencies"],
  "helm-requirements": {
    "fileMatch": ["\\Chart.yaml$"],
    "aliases": {
      "hmctspublic": "https://hmctspublic.azurecr.io/helm/v1/repo/"
    }
  },
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    }
  ]
}
