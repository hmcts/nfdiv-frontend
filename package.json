    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/glob": "^7.1.4",
    "@types/lockfile": "^1.0.2",
    "@types/lodash": "^4.14.175",
    "@types/multer": "^1.4.7",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^16.10.1",
