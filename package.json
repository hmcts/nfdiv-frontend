{
  "name": "nfdiv-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.15.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "NODE_ENV=production yarn node src/main/server.js",
    "start:dev": "NODE_ENV=development yarn start:watch",
    "start:docker": "NODE_ENV=docker yarn start:watch",
    "start:docker:civil": "NODE_ENV=docker FORCE_CIVIL_MODE=true yarn start:watch",
    "start:watch": "nodemon --config nodemon.express.json",
    "lint": "tsc --noEmit -p tsconfig.eslint.json && eslint . --ext .js,.ts",
    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "build:ts": "tsc -P . --outDir ./src/main",
    "test": "npx if-env CI=true && exit 0 || yarn test:unit",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:a11y": "echo 'nope' && exit 0",
    "test:pa11y": "jest -c jest.a11y.config.js --maxWorkers 1",
    "test:cucumber": "yarn test:parallel '^(?!.*@flaky)' ; yarn merge-reports",
    "test:cucumber:docker": "yarn test:parallel:docker '^(?!.*@flaky)' ; yarn merge-reports",
    "test:e2e": "yarn test:parallel '^(?!.*(@nightly|@flaky))' ; yarn merge-reports",
    "test:e2e:docker": "yarn test:parallel:docker '^(?!.*(@nightly|@flaky))' ; yarn merge-reports",
    "test:parallel": "codeceptjs run-workers 20 --config ./src/test/functional/codecept.conf.js --reporter mocha-multi --grep",
    "test:parallel:docker": "NODE_ENV=docker codeceptjs run-workers 10 --config ./src/test/functional/codecept.conf.js --reporter mocha-multi --grep",
    "test:nightly": "yarn test:parallel @nightly ; yarn merge-reports",
    "test:nightly:docker": "yarn test:parallel:docker @nightly ; yarn merge-reports",
    "merge-reports": "mochawesome-merge ./temp-reports/*.json > './functional-output/functional/reports/Functional test report.json' && marge './functional-output/functional/reports/Functional test report.json' -f 'Functional test report' -o ./functional-output/functional/reports --inline && rm -rf ./temp-reports",
    "test:cucumber:grep": "TEST_HEADLESS=false yarn test:cucumber:headless:grep",
    "test:cucumber:docker:grep": "NODE_ENV=docker yarn test:cucumber:grep",
    "test:cucumber:headless:grep": "codeceptjs run --config ./src/test/functional/codecept.conf.js --steps --verbose --reporter mocha-multi --grep",
    "test:cucumber:headless:docker:grep": "NODE_ENV=docker yarn test:cucumber:headless:grep",
    "test:cucumber:reports": "npx allure-commandline serve functional-output/functional/reports",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "jest -c jest.smoke.config.js",
    "test:functional": "yarn test:e2e",
    "test:fullfunctional": "run-p test:cucumber test:pa11y",
    "test:crossbrowser": "yarn test:crossbrowser:playwright",
    "test:crossbrowser:playwright": "npx playwright install chromium firefox webkit && yarn test:crossbrowser:codeceptjs --reporter mochawesome --grep '^(?!.*@flaky)'",
    "test:crossbrowser:playwright:docker": "docker pull mcr.microsoft.com/playwright:focal && docker run --rm --volume $PWD:/home/app --workdir='/home/app' --env TEST_PASSWORD --env TEST_URL mcr.microsoft.com/playwright:focal bash -x src/test/docker-yarn.sh test:crossbrowser:playwright $UID",
    "test:crossbrowser:sauce": "SAUCE='true' yarn test:crossbrowser:codeceptjs run --grep '^(?!.*@flaky)'",
    "test:crossbrowser:codeceptjs": "codeceptjs run --config ./src/test/cross-browser/codecept.conf.js",
    "cichecks": "yarn && run-p build lint test",
    "fortifyScan": "./src/test/java/gradlew -p ./src/test/java fortifyScan"
  },
