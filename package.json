{
  "name": "nfdiv-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "nodemon --config nodemon.json",
    "lint": "eslint . --ext .js,.ts",
    "build": "webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:a11y": "echo 'nope' && exit 0",
    "test:pa11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.a11y.config.js --maxWorkers 15",
    "test:cucumber": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run-workers 8 parallel --features --verbose",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "yarn test:cucumber && yarn test:pa11y",
    "test:crossbrowser": "concurrently \"yarn test:crossbrowser:latest:docker\" \"yarn test:crossbrowser:ie\" --names latest,ie",
    "test:crossbrowser:latest": "yarn test:crossbrowser:codeceptjs run-multiple crossBrowser --features --verbose",
    "test:crossbrowser:latest:docker": "docker run --rm --volume $PWD:/home/app --workdir=\"/home/app\" --env TEST_URL mcr.microsoft.com/playwright:bionic yarn test:crossbrowser:latest",
    "test:crossbrowser:ie": "IE='true' yarn test:crossbrowser:codeceptjs run --features --verbose",
    "test:crossbrowser:codeceptjs": "codeceptjs --config ./src/test/cross-browser/codecept.conf.js",
    "cichecks": "yarn && yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@hmcts/info-provider": "^1.0.0",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/nodejs-logging": "^3.0.2",
    "@hmcts/properties-volume": "^0.0.9",
    "@types/autobind-decorator": "^2.1.0",
    "@types/axios": "^0.14.0",
    "@types/config": "^0.0.38",
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/csurf": "^1.9.36",
    "@types/es6-promisify": "^6.0.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/glob": "^7.1.3",
    "@types/helmet": "^4.0.0",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.26",
    "@types/nunjucks": "^3.1.4",
    "@types/redis": "^2.8.28",
    "@types/require-directory": "^2.1.1",
    "@types/serve-favicon": "^2.5.2",
    "@types/session-file-store": "^1.2.1",
    "applicationinsights": "^1.8.9",
    "autobind-decorator": "^2.4.0",
    "awilix": "^4.3.1",
    "axios": "^0.21.1",
    "config": "^3.3.3",
    "connect-redis": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "express": "^4.17.1",
    "express-nunjucks": "^2.2.5",
    "express-session": "^1.17.1",
    "glob": "^7.1.6",
    "govuk-frontend": "^3.11.0",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.20",
    "nunjucks": "^3.2.2",
    "redis": "^3.0.2",
    "require-directory": "^2.1.1",
    "serve-favicon": "^2.5.0",
    "session-file-store": "^1.5.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
