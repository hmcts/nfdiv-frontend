    "@uppy/file-input": "^2.0.5",
    "@uppy/progress-bar": "^2.0.4",
    "@uppy/xhr-upload": "^2.0.7",
    "applicationinsights": "^2.2.0",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.25.0",
