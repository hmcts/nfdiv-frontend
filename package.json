{
  "name": "nfdiv-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=14.8.1"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "NODE_ENV=production node src/main/server.js",
    "start:dev": "NODE_ENV=development yarn start:watch",
    "start:docker": "NODE_ENV=docker yarn start:watch",
    "start:watch": "nodemon --config nodemon.express.json",
    "lint": "tsc --noEmit -p tsconfig.eslint.json && eslint . --ext .js,.ts",
    "build": "webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "build:ts": "tsc -P . --outDir ./src/main",
    "test": "npx if-env ${CI} && yarn test:unit || exit 0",
    "test:unit": "jest",
    "test:coverage": "jest --coverage",
    "test:a11y": "echo 'nope' && exit 0",
    "test:pa11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.a11y.config.js --maxWorkers 1",
    "test:cucumber": "yarn test:parallel '^(?!.*@flaky)'",
    "test:e2e": "yarn test:parallel '^(?!.*(@nightly|@flaky))'",
    "test:parallel": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run-workers 8 parallel --grep",
    "test:cucumber:grep": "TEST_HEADLESS=false NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run --steps --verbose --grep",
    "test:cucumber:grep:docker": "NODE_ENV=docker yarn test:cucumber:grep",
    "test:cucumber:reports": "npx allure-commandline serve functional-output/functional/reports",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "yarn test:e2e",
    "test:fullfunctional": "run-p test:cucumber test:pa11y",
    "test:crossbrowser": "yarn test:crossbrowser:sauce",
    "test:crossbrowser:playwright": "yarn test:crossbrowser:codeceptjs run-multiple crossBrowser --grep '^(?!.*@flaky)'",
    "test:crossbrowser:playwright:docker": "docker pull mcr.microsoft.com/playwright:focal && docker run --rm --volume $PWD:/home/app --workdir='/home/app' --env TEST_PASSWORD --env TEST_URL mcr.microsoft.com/playwright:focal bash -x src/test/docker-yarn.sh test:crossbrowser:playwright $UID",
    "test:crossbrowser:sauce": "SAUCE='true' yarn test:crossbrowser:codeceptjs run --grep '^(?!.*@flaky)'",
    "test:crossbrowser:codeceptjs": "codeceptjs --config ./src/test/cross-browser/codecept.conf.js",
    "cichecks": "yarn && run-p build lint test"
  },
  "dependencies": {
    "@hmcts/cookie-manager": "^1.0.0",
    "@hmcts/frontend": "^0.0.50-alpha",
    "@hmcts/nodejs-healthcheck": "^1.7.3",
    "@hmcts/nodejs-logging": "^4.0.4",
    "@hmcts/properties-volume": "^0.0.14",
    "@types/config": "^3.3.0",
    "@types/connect-redis": "^0.0.18",
    "@types/cookie-parser": "^1.4.3",
    "@types/csurf": "^1.11.2",
    "@types/es6-promisify": "^6.0.1",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.4",
    "@types/glob": "^8.0.0",
    "@types/lodash": "^4.14.185",
    "@types/multer": "^1.4.7",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^18.7.23",
    "@types/nunjucks": "^3.2.1",
    "@types/require-directory": "^2.1.2",
    "@types/serve-favicon": "^2.5.3",
    "@types/session-file-store": "^1.2.2",
    "@types/toobusy-js": "^0.5.2",
    "@types/uuid": "^8.3.4",
    "@uppy/core": "^2.3.3",
    "@uppy/drop-target": "^1.1.4",
    "@uppy/file-input": "^2.1.2",
    "@uppy/progress-bar": "^2.1.2",
    "@uppy/xhr-upload": "^2.1.3",
    "applicationinsights": "^2.3.5",
    "autobind-decorator": "^2.4.0",
    "axios": "^0.27.2",
    "config": "^3.3.8",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "dayjs": "^1.11.5",
    "email-validator": "^2.0.4",
    "express": "^4.18.1",
    "express-http-proxy": "^1.6.3",
    "express-nunjucks": "^2.2.5",
    "express-session": "^1.17.3",
