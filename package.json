    "@types/express-session": "^1.17.3",
    "@types/glob": "^7.1.3",
    "@types/helmet": "^4.0.0",
    "@types/lockfile": "^1.0.1",
    "@types/lodash": "^4.14.170",
    "@types/multer": "^1.4.5",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^15.6.1",
