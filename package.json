{
  "name": "nfdiv-frontend",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "start": "NODE_ENV=production ts-node -r tsconfig-paths/register src/main/server.ts",
    "start:dev": "NODE_ENV=development nodemon --config nodemon.json",
    "start:docker": "NODE_ENV=docker nodemon --config nodemon.json",
    "lint": "tsc --noEmit -p tsconfig.eslint.json && eslint . --ext .js,.ts",
    "build": "webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:a11y": "echo 'nope' && exit 0",
    "test:pa11y": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.a11y.config.js --maxWorkers 15",
    "test:cucumber": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run-workers 8 parallel --grep '^(?!.*@flaky)'",
    "test:e2e": "NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run-workers 8 parallel --grep '^(?!.*(@nightly|@flaky))'",
    "test:cucumber:grep": "TEST_HEADLESS=false NODE_TLS_REJECT_UNAUTHORIZED=0 codeceptjs --config ./src/test/functional/codecept.conf.js run --steps --grep",
    "test:cucumber:reports": "npx allure-commandline serve functional-output/functional/reports",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "NODE_TLS_REJECT_UNAUTHORIZED=0 jest -c jest.smoke.config.js",
    "test:functional": "yarn test:e2e && yarn test:pa11y",
    "test:fullfunctional": "yarn test:cucumber && yarn test:pa11y",
    "test:crossbrowser": "concurrently \"yarn test:crossbrowser:latest:docker\" \"yarn test:crossbrowser:ie\" --names latest,ie",
    "test:crossbrowser:latest": "yarn test:crossbrowser:codeceptjs run-multiple crossBrowser --grep '^(?!.*@flaky)'",
    "test:crossbrowser:latest:docker": "docker run --rm --volume $PWD:/home/app --workdir=\"/home/app\" --env TEST_URL mcr.microsoft.com/playwright:bionic bash -c \"yarn test:crossbrowser:latest; chown -R $UID:$UID functional-output\"",
    "test:crossbrowser:ie": "IE='true' yarn test:crossbrowser:codeceptjs run --grep '^(?!.*@flaky)'",
    "test:crossbrowser:codeceptjs": "codeceptjs --config ./src/test/cross-browser/codecept.conf.js",
    "cichecks": "yarn && yarn build && yarn lint && yarn test"
  },
  "dependencies": {
    "@hmcts/info-provider": "^1.0.0",
    "@hmcts/nodejs-healthcheck": "^1.7.0",
    "@hmcts/nodejs-logging": "^3.0.2",
    "@hmcts/properties-volume": "^0.0.9",
