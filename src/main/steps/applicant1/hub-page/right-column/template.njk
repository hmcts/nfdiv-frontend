<hr class="govuk-section-break govuk-section-break--l govuk-section-break--additional">
<h2 class="govuk-heading-s">{{ iWantTo }}</h2>

<p class="govuk-body"><a class="govuk-link" href={{applicationDownload.reference}}
download={{applicationDownload.name}}>{{applicationDownload.link}}</a></p>
{% if aosSubmitted %}
  <p class="govuk-body"><a class="govuk-link" href={{respondentAnswersDownload.reference}}
   download={{respondentAnswersDownload.name}}>{{respondentAnswersDownload.link}}</a></p>
{% endif %}
{% if hasCertificateOfService %}
  <p class="govuk-body"><a class="govuk-link" href={{certificateOfServiceDownload.reference}}
   download={{certificateOfServiceDownload.name}}>{{certificateOfServiceDownload.link}}</a></p>
{% endif %}
{% if hasCertificateOfDeemedOrDispensedService %}
  <p class="govuk-body"><a class="govuk-link" href={{deemedOrDispensedDownload.reference}}
   download={{deemedOrDispensedDownload.name}}>{{deemedOrDispensedDownload.link}}</a></p>
{% endif %}
{% if hasCertificateOfEntitlement %}
  <p class="govuk-body"><a class="govuk-link" href={{certificateOfEntitlementDownload.reference}}
   download={{certificateOfEntitlementDownload.name}}>{{certificateOfEntitlementDownload.link}}</a></p>
{% endif %}

<p class="govuk-body">{{ reviewContactDetails | safe }}</p>
<hr class="govuk-section-break govuk-section-break--l govuk-section-break--visible">
