{% from "govuk/components/button/macro.njk" import govukButton %}

{% extends "common/page.njk" %}

{% block content %}
  <h1 class="govuk-heading-xl">{{ title }}</h1>
  <p class="govuk-body">{{ checkYourAnswers }}</p>
  <pre>
{{ formState | json(2) }}
  </pre>
  {{ govukButton({
  text: submitApplication,
  preventDoubleClick: true
  }) }}
  <a href="/summary/reset" class="govuk-link govuk-button--warning">{{ resetApplication }}</a>
{% endblock %}
